  "dependencies": {
    "@js-entity-repos/core": "^6.0.2",
    "@js-entity-repos/memory": "^3.0.1",
    "async-reactor": "^1.2.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "todomvc-app-css": "^2.1.0"
  },
  "devDependencies": {
    "@ht2-labs/semantic-release": "1.0.28",
    "@ht2-labs/typescript-project": "1.0.9",
    "@types/mocha": "2.2.48",
    "@types/power-assert": "1.4.29",
    "@types/react": "16.0.38",
    "@types/react-dom": "16.0.4",
    "assert-rejects": "0.1.1",
    "jscpd": "0.6.17",
    "mocha": "5.0.1",
    "nyc": "11.4.1",
    "parcel-bundler": "1.6.1",
    "power-assert": "1.4.4",
    "rimraf": "2.6.2"
  },
  "publishConfig": {
    "access": "public"
