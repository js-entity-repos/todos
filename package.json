    "react-dom": "^16.2.0",
    "todomvc-app-css": "^2.1.0"
  },
  "devDependencies": {
    "@ht2-labs/semantic-release": "1.0.31",
    "@ht2-labs/typescript-project": "1.0.9",
    "@types/mocha": "5.0.0",
    "@types/power-assert": "1.4.29",
    "@types/prop-types": "15.5.2",
    "@types/react": "16.3.5",
    "@types/react-dom": "16.0.4",
    "assert-rejects": "0.1.1",
    "gh-pages": "1.1.0",
    "jscpd": "0.6.17",
    "mocha": "5.0.5",
    "nyc": "11.6.0",
    "parcel-bundler": "1.7.0",
    "power-assert": "1.5.0",
    "rimraf": "2.6.2",
    "typescript": "2.9.0-dev.20180405"
  },
  "publishConfig": {
    "access": "public"
  }
